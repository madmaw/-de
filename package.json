{
  "devDependencies": {
    "@dprint/dockerfile": "=0.3.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.13.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-dprint-integration": "^0.3.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-no-autofix": "^2.0.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "turbo": "^2.2.1",
    "typescript": "~5.5.3",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.3",
    "vitest-mock-extended": "^2.0.2"
  },
  "license": "MIT",
  "main": "index.js",
  "name": "tscriptors",
  "packageManager": "yarn@1.22.22",
  "private": true,
  "resolutions": {
    "**/**/@dprint/dockerfile": "=0.3.0",
    "//": "vitest.workspace.ts gets incompatible plugin types without forcing vite version to be the same everywhere. eslint/dprint needs the dockerfile set to this version or you get a runtime error"
  },
  "scripts": {
    "check-types": "turbo run check-types",
    "lint": "turbo run lint --continue && yarn lint:root",
    "lint:fix": "turbo run lint:fix --continue && yarn lint:root:fix",
    "lint:root": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint --max-warnings=0 *.cjs *.json",
    "lint:root:fix": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint --fix *.cjs *.json",
    "test": "turbo run test"
  },
  "version": "0.0.1",
  "workspaces": {
    "packages": [
      "packages/*",
      "support/*"
    ]
  }
}
